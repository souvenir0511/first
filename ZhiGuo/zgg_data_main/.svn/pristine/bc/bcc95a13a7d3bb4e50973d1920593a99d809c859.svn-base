
package com.zgg.batch.entity;

import lombok.Data;

import java.io.Serializable;
import java.math.BigDecimal;
import java.sql.Timestamp;
import java.util.Date;

@Data
public class PolicyMysql implements Serializable {

    /** 主键 */
    private String id;

    /** 项目名称 */
    private String projectName;

    /** 政策级别 */
    private String policyLevel;

    /** 数据来源类型 */
    private String sourceType;

    /** 省ID */
    private String provinceId;

    /** 省名称 */
    private String provinceName;

    /** 市ID */
    private String cityId;

    /** 市名称 */
    private String cityName;

    /** 区ID */
    private String areaId;

    /** 区名称 */
    private String areaName;

    /** 申报开始时间 */
    private String declareStartTime;

    /** 申报结束时间 */
    private String declareEndTime;

    /** 技术领域 */
    private String techField;

    /** 项目类别 */
    private String projectType;

    /**资金扶持力度*/
    private BigDecimal financialSupport;

    /** 政府部门名称 */
    private String govDepartmentName;

    /** 申报条件 */
    private String declareCondition;

    /** 支持力度 */
    private String supportDegree;

    /** 申报材料及方式 */
    private String declareMaterialMethod;

    /** 政策状态（申报中、即将开始、已结束） */
    private String policyStatus;

    /** 官方发布时间 */
    private String officialPublishTime;

    /** 爬取时间 */
    private Timestamp crawlTime;

    /** 发布状态（1已发布，0未发布） */
    private Boolean status;

    /** 是否置顶（1置顶，0未置顶） */
    private Boolean topFlag;

    /** 数据来源名称 */
    private String sourceInfo;

    /** 删除标志 */
    private Boolean deleteFlag;

    /** 创建者 */
    private Long createUserId;

    /** 创建时间 */
    private Date createTime;

    /** 更新者 */
    private Long updateUserId;

    /** 更新时间 */
    private Date updateTime;
}
